                    @{
                        BookShopContext db = new BookShopContext();
                        var Artical = db.Articals.Where(a => a.ArticalId == 1).Select(a => a);
                        foreach (var g in Artical)
                        {
                            string path = g.ArticalTitle + ".jpg";
                            <table>
                                <tr><td> 
                                        <div style="text-align:center">
                                            <img src="~/Image/ArticalPicture/@path" style="width:80%; max-width:100%;height=100" title="@g.ArticalTitle" />
                                        </div>
                                        <span>@g.ArticalTitle</span>  <span  style="color: gray; font-style: oblique;font-size: 60%;text-align: right;">更新日期：@g.ArticalDate</span>
                                    </td></tr>
                            </table>
                        }
                        var detail = db.ArticalToBookDetails.Where(d => d.ArticalId == 1).Select(d => new{d.Book.BookTitle,d.Book.UnitPrice, d.Book.CoverPath, d.Book.BookId, });
                        foreach (var b in detail)
                            {
                                //TODO：優惠價
                                var q3 = db.AuthorDetails.Where(a => a.BookId == b.BookId).Select(a => new { a.Author.AuthorName });
                                string s = "";
                                <div >
                                    <div style="position:relative">
                                    <a id="@b.BookId"><img src="@b.CoverPath" class="d-block w-100" alt="..." @b.CoverPath /></a>
                                    </div>
                                    <div class="caption bookCaption">
                                        <ul style="list-style:none; margin:0; padding:0;">
                                            <li class='prod_TitleMain'> <a id="@b.BookId"> @b.BookTitle</a> </li>
                                            <li class="discPrice">原價：<del style="color:red;font-style:oblique">@b.UnitPrice</del></li>
                                            <li class="discPrice" style="background-color:wheat">優惠價：<span>79</span>折<span>284</span>元</li>
                                        </ul>
                                    </div>
                                </div>
                            }
                        }
                    }
